#=========================== Filebeat inputs =============================
filebeat.inputs:
- type: log
  enabled: true
  paths:
    - {{ harmony_root_path }}/latest/zerolog*.log
  json.keys_under_root: true
  json.overwrite_keys: true

#----------------------------- Logstash output --------------------------------
output.logstash:
  hosts: ["{{ logstash_ip }}:5045"]
